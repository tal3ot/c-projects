#include <stdio.h>
#include <stdlib.h>
#include <assert.h>

int main (){
    int  row = 5, colomn = 5,i,j;
    int arry[row][colomn] ;
    printf("plz fill this matrix 5 x 5 \n");
    for ( i = 0; i < row; i++){
            printf("Entre The 5 num of %d Row \n", i);
        for ( j = 0; j < colomn; j++){

            scanf("%d", &arry[i][j]);
        }
    }
  // finding the sum of each row
    printf("\n The sum of The 1st Row is %d", arry[0][0] + arry[0][1] + arry[0][2] + arry[0][3] + arry[0][4]);
    printf("\n The sum of The 2nd Row is %d", arry[1][0] + arry[1][1] + arry[1][2] + arry[1][3] + arry[1][4]);
    printf("\n The sum of The 3rd Row is %d", arry[2][0] + arry[2][1] + arry[2][2] + arry[2][3] + arry[2][4]);
    printf("\n The sum of The 4rd Row is %d", arry[3][0] + arry[3][1] + arry[3][2] + arry[3][3] + arry[3][4]);
    printf("\n The sum of The 5rd Row is %d", arry[4][0] + arry[4][1] + arry[4][2] + arry[4][3] + arry[4][4]);
    // finding the sum of each colomn
    printf("\n\n\n The sum of The 1st colomn is %d", arry[0][0] + arry[1][0] + arry[2][0] + arry[3][0] + arry[4][0]);
    printf("\n The sum of The 2nd colomn is %d", arry[0][1] + arry[1][1] + arry[2][1] + arry[3][1] + arry[4][1]);
    printf("\n The sum of The 3rd colomn is %d", arry[0][2] + arry[1][2] + arry[2][2] + arry[3][2] + arry[4][2]);
    printf("\n The sum of The 4rd colomn is %d", arry[0][3] + arry[1][3] + arry[2][3] + arry[3][3] + arry[4][3]);
    printf("\n The sum of The 5rd colomn is %d", arry[0][4] + arry[1][4] + arry[2][4] + arry[3][4] + arry[4][4]);
    
    printf("\n%d", (sizeof arry) / (sizeof arry [0][0]));

}

