#include <stdio.h>
#include <stdlib.h>
#include <assert.h>

int main (){
    unsigned int a, b, result, i, n;
    a = 0;
    b = 1;

    printf(" Entre the num of terms : " );
    scanf("%u", &n);
    for ( i = 0; i <= n; i++ ){
        printf(" %u", a);
        result = a + b;
        a = b;
        b = result;
    }

}

